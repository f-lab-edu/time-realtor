{
  "name": "Zipsoon Development",
  "dockerComposeFile": ["../docker-compose.yml", "../docker-compose.local.yml"],
  "service": "api",
  "workspaceFolder": "/workspaces/zipsoon",
  "containerEnv": {
    "LOCAL_DB_NAME": "zipsoon",
    "LOCAL_DB_USERNAME": "zipsoon",
    "LOCAL_DB_PASSWORD": "zipsoon",
    "LOCAL_JWT_SECRET": "test-only-secret-key-for-development-environment",
    "LOCAL_SPRING_DATASOURCE_URL": "jdbc:postgresql://db:5432/zipsoon",
    "NAVER_LAND_BASE_URL": "https://new.land.naver.com/api",
    "NAVER_LAND_AUTH_TOKEN": "test-token"
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "vscjava.vscode-java-pack",
        "vscjava.vscode-gradle",
        "redhat.vscode-yaml"
      ]
    }
  },
  "features": {
    "ghcr.io/devcontainers/features/java:1.6.0": {
      "version": "17",
      "installGradle": true
    }
  },
  "forwardPorts": [8080, 5432]
}
